public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    Integer max = 0;
    Integer count = 0;

    for (Integer i = 0; i < numbers.size(); i++) {
        count++;
        if (numbers[i] == 0) {
            count = 0;
        } else if (count > max) {
            max = count;
            }
    }
    return count;
}