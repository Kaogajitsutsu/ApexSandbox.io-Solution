public Integer fibonacci(Integer n) {
    //code here
    Integer first = 1;
    Integer second = 1;
    Integer sumFib = 2;

    if (n < 3) {
        return sumFib - 1;
    }
    
    for (Integer i = 3; i < n; i++) {
        first = second;
        second = sumFib;
        sumFib = first + second;
    }

    return sumFib;
}