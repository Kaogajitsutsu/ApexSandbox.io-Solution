public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	//code here
    List<Integer> numbers = new List<Integer>{num1, num2, num3};
        Integer largest = 0;
        For (Integer i = 0; i < numbers[i]; i++) {
            If (numbers[i] > largest) {
                largest = numbers[i];
            }
        }
        return largest;
}