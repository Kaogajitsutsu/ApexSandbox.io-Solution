public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c student = new apxio__Student__c();
    student.Name = name;
    student.apxio__Email__c = email;
    student.apxio__Phone__c = phone;

    insert student;

    String registrationNumber;
    List<apxio__Student__c> records = [SELECT apxio__Registration_Number__c FROM apxio__Student__c WHERE Id = student.Id LIMIT 1];
    registrationNumber = records[0].apxio__Registration_Number__c;

    return registrationNumber;
}