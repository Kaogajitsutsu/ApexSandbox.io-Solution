public Integer nextPrime(Integer num) {
    //code here
    public Boolean isPrime(Integer num) {
        for (Integer i = 2; i <= (num - 1); i++) {
            if (Math.mod(num, i) == 0) {
                return false;
            } 
        }
        return true;
    }

    for (Integer j = (num + 1); j <= (num += num); j++) {
        if (isPrime(j) == true) {
            return j;
        }
    }

}